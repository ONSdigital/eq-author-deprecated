{
  "name": "eq-author",
  "version": "0.1.0",
  "description": "ONS eQ Author tool",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ONSdigital/eq-author"
  },
  "license": "MIT",
  "scripts": {
    "server": "nodemon data/server.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "prebuild": "npm run test && npm run build:clean",
    "build:clean": "rimraf ./build/*",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p",
    "analyze": "node ./internals/scripts/analyze.js",
    "start": "npm run start:tunnel",
    "start:tunnel": "cross-env NODE_ENV=development node server",
    "start:prod": "cross-env NODE_ENV=production node server",
    "pagespeed": "node ./internals/scripts/pagespeed.js",
    "presetup": "npm i chalk",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint . --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:js:staged": "eslint-staged",
    "lint:css": "stylelint ./qBuilder/assets/**/*.css",
    "lint:css:staged": "stylelint-staged",
    "lint:staged": "npm run lint:js:staged && npm run lint:css:staged",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test karma start internals/testing/karma.conf.js --single-run",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "test:firefox": "npm run test -- --browsers Firefox",
    "test:safari": "npm run test -- --browsers Safari",
    "test:ie": "npm run test -- --browsers IE",
    "coveralls": "cat ./coverage/lcov/lcov.info | coveralls"
  },
  "pre-commit": "lint:staged",
  "babel": {
    "presets": [
      "es2015-webpack",
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "only": [
          "qBuilder/assets"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.7.4",
    "chalk": "^1.1.3",
    "compression": "^1.6.1",
    "directory-tree": "^1.0.0",
    "express": "^4.13.4",
    "fontfaceobserver": "^1.7.1",
    "history": "^2.1.0",
    "immutable": "^3.8.1",
    "js-beautify": "^1.6.3",
    "react": "^15.0.1",
    "react-codemirror": "^0.2.6",
    "react-dom": "^15.0.1",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "react-router-redux": "^4.0.4",
    "react-router-scroll": "^0.2.0",
    "redux": "^3.5.2",
    "redux-immutable": "^3.0.6",
    "redux-saga": "^0.10.0",
    "reselect": "^2.5.1",
    "sanitize.css": "^3.3.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-react-constant-elements": "^6.8.0",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-webpack": "^6.4.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.15.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.2",
    "cheerio": "^0.20.0",
    "coveralls": "^2.11.9",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.10.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-config-standard-react": "^2.4.0",
    "eslint-import-resolver-webpack": "^0.2.4",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-standard": "^1.3.2",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.18.0",
    "expect-jsx": "^2.5.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-mock": "^4.5.4",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.0",
    "imports-loader": "^0.6.5",
    "ip": "^1.1.2",
    "isparta": "^4.0.0",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "json-server": "^0.8.14",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.3",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "lint-staged": "^0.2.1",
    "mocha": "^2.4.5",
    "ngrok": "2.1.8",
    "nodemon": "^1.9.2",
    "null-loader": "^0.1.1",
    "offline-plugin": "^3.2.0",
    "plop": "^1.5.0",
    "postcss-color-function": "^2.0.1",
    "postcss-cssnext": "^2.5.2",
    "postcss-focus": "^1.0.0",
    "postcss-loader": "^0.9.1",
    "postcss-modules-values": "^1.1.3",
    "postcss-reporter": "^1.3.3",
    "postcss-scss": "^0.1.8",
    "precss": "^1.4.0",
    "psi": "^2.0.3",
    "redux-logger": "^2.6.1",
    "redux-mock-store": "^1.1.0",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.2",
    "shelljs": "^0.7.0",
    "sinon": "^2.0.0-pre",
    "style-loader": "^0.13.1",
    "stylelint": "^6.3.3",
    "stylelint-config-standard": "^8.0.0",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.8",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
